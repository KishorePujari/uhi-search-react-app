{
  "name": "hackathon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "eslint": "eslint src",
    "prettier": "prettier -c 'src/**/*.js'",
    "prettier:json": "prettier -c 'src/assets/*.json'",
    "format": "prettier --write 'src/**/*.js'",
    "format:json": "prettier --write 'src/assets/*.json'",
    "quality": "npm run format && npm run format:json && npm run eslint",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm i && sh pullconfig.sh && npm run eslint && npm run test:coverage",
    "local:ci": "npm i && sh pullconfig.sh",
    "start": "webpack-dev-server --open --hot --port 3000 --mode development",
    "debug": "webpack --config webpack.debug.config.js",
    "production": "webpack --mode production --config webpack.prod.config.js"
  },
  "keywords": [],
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "echo Hello .."
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "canvas": "2.6.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-canvas-mock": "^2.3.0",
    "jest-cli": "^25.1.0",
    "jest-when": "^3.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-rtl": "^1.7.3",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "socket.io": "^2.3.0",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^6.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@material-table/core": "^2.3.39",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "axios": "^0.21.1",
    "chart.js": "^3.2.1",
    "date-fns": "^2.24.0",
    "google-maps-react": "^2.0.6",
    "lodash.debounce": "^4.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.32.12",
    "terra-responsive-element": "^5.34.0",
    "web-vitals": "^1.0.1"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "text-summary",
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/aggregated-translations/**",
      "!**/tests/**",
      "!**/serviceWorker.js",
      "!**/jestglobalsetup.js",
      "!**/jestsetup.js",
      "!**/postcss.config.js",
      "!**/webpack.config.js",
      "!**/webpack.debug.config.js",
      "!**/webpack.prod.config.js",
      "!**/index.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg|png)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "aggregated-translations",
      "node_modules"
    ]
  }
}
